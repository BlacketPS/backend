model resource {
  id                                 Int       @id @default(autoincrement())
  path                               String    @db.VarChar(255)
  createdAt                          DateTime  @db.Timestamptz(6) @default(now())
  updatedAt                          DateTime  @db.Timestamptz(6) @default(now())
  banner                             banner[]
  blook_blook_backgroundIdToresource blook[]   @relation("blook_backgroundIdToresource")
  blook_blook_imageIdToresource      blook[]   @relation("blook_imageIdToresource")
  emoji                              emoji[]
  font                               font[]
  group                              group[]
  guild                              guild[]
  item                               item[]
  pack                               pack[]
  product                            product[]
  user_user_avatarIdToresource       user[]    @relation("user_avatarIdToresource")
  user_user_bannerIdToresource       user[]    @relation("user_bannerIdToresource")
  user_user_customAvatarIdToresource user[]    @relation("user_customAvatarIdToresource")
  user_user_customBannerIdToresource user[]    @relation("user_customBannerIdToresource")
}
