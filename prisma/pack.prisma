model pack {
  id         Int      @id @default(autoincrement())
  name       String   @unique @db.VarChar(255)
  price      Int      @default(0)
  enabled    Boolean  @default(true)
  imageId    Int
  innerColor String   @db.VarChar(255)
  outerColor String   @db.VarChar(255)
  priority   Int      @default(0)
  createdAt  DateTime @db.Timestamptz(6) @default(now())
  updatedAt  DateTime @db.Timestamptz(6) @default(now())
  blook      blook[]
  resource   resource @relation(fields: [imageId], references: [id], onDelete: NoAction)
}
