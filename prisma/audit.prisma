model audit {
  id                               Int              @id @default(autoincrement())
  userId                           String           @db.VarChar(255)
  secondaryUserId                  String?          @db.VarChar(255)
  action                           Int
  consoleCommand                   String?          @db.VarChar(255)
  punishmentId                     Int?
  groupId                          Int?
  ipAddressId                      Int
  createdAt                        DateTime         @db.Timestamptz(6)
  updatedAt                        DateTime         @db.Timestamptz(6)
  oldUsername                      String?          @db.VarChar(255)
  newUsername                      String?          @db.VarChar(255)
  group                            group?           @relation(fields: [groupId], references: [id])
  ip_address                       ip_address       @relation(fields: [ipAddressId], references: [id], onDelete: Cascade)
  user_punishment                  user_punishment? @relation(fields: [punishmentId], references: [id])
  user_audit_secondaryUserIdTouser user?            @relation("audit_secondaryUserIdTouser", fields: [secondaryUserId], references: [id])
  user_audit_userIdTouser          user             @relation("audit_userIdTouser", fields: [userId], references: [id], onDelete: Cascade)
}
