model rarity {
  id            Int      @id @default(autoincrement())
  name          String   @unique @db.VarChar(255)
  color         String   @db.VarChar(255)
  animationType Int
  experience    Int
  createdAt     DateTime @db.Timestamptz(6)
  updatedAt     DateTime @db.Timestamptz(6)
  blook         blook[]
  item          item[]
}
