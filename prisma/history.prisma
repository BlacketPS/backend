model history {
  id                                 Int      @id @default(autoincrement())
  previousOwnerId                    String?  @db.VarChar(255)
  newOwnerId                         String?  @db.VarChar(255)
  type                               Int      @default(1)
  createdAt                          DateTime @db.Timestamptz(6) @default(now())
  updatedAt                          DateTime @db.Timestamptz(6) @default(now())
  user_history_newOwnerIdTouser      user?    @relation("history_newOwnerIdTouser", fields: [newOwnerId], references: [id], onDelete: NoAction)
  user_history_previousOwnerIdTouser user?    @relation("history_previousOwnerIdTouser", fields: [previousOwnerId], references: [id], onDelete: NoAction)
}
